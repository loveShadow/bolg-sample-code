<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.blogsamplecode">

    <uses-permission android:name="android.permission.VIBRATE"/>

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:theme="@style/AppTheme">
        <activity android:name=".main.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <!-- Notification 测试 -->
        <activity android:name=".studyCode.NotificationSimple.NotificationActivity"/>
    </application>

</manifest>
